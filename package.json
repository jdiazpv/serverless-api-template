{
  "name": "serverless-api-typescript",
  "version": "2.1.0",
  "description": "Serverless API example using Typescript",
  "scripts": {
    "deploy": "dotenv -- sls deploy",
    "remove": "dotenv -- sls remove",
    "test": "jest --coverage",
    "watch-tests": "jest --watch",
    "e2e-test": "jest -c=jest.config.e2e.js",
    "load-data": "dotenv -- sls invoke -f load-sample-data",
    "serve-docs": "live-server docs",
    "generate-docs": "swagger-jsdoc -d docs/swagger-spec.yml -o docs/swagger.yml",
    "codecov": "codecov -f coverage/*.json"
  },
  "dependencies": {
    "@manwaring/lambda-wrapper": "^1.2.2",
    "class-validator": "^0.11.0",
    "source-map-support": "^0.5.16",
    "utility-types": "^3.10.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@serverless/event-mocks": "^1.1.1",
    "@types/aws-lambda": "^8.10.39",
    "@types/chance": "^1.0.8",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^3.4.6",
    "aws-sdk": "^2.597.0",
    "babel-jest": "^24.9.0",
    "chance": "^1.1.4",
    "codecov": "^3.6.1",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^2.7.0",
    "jest": "^24.9.0",
    "live-server": "^1.2.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "rimraf": "^3.0.0",
    "serverless": "^1.60.4",
    "serverless-cloudformation-resource-counter": "^1.0.2",
    "serverless-lumigo": "^1.6.1",
    "serverless-plugin-iam-checker": "^1.0.1",
    "serverless-plugin-test-helper": "^1.3.1",
    "serverless-prune-plugin": "^1.4.0",
    "serverless-webpack": "^5.2.0",
    "swagger-jsdoc": "^3.5.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "watch": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run generate-docs"
    }
  },
